using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
            int a = Convert.ToInt32(Console.ReadLine());
            if (a > 2)
            {
                Console.WriteLine(a);
            } else
            {
                Console.WriteLine(1);
            }
        }
    }
}
